For Assembler programmers:

FRAMES:
 .BYTE 4
HEIGHT:
 .BYTE 30
GAP:
 .BYTE 0
P0COLOURS:
 .BYTE $1a,$1a,$1a,$1a
P1COLOURS:
 .BYTE $24,$24,$24,$24
P0DATA:
; FRAME 1
 .BYTE $00,$00,$0c,$0c,$0c,$0c,$0c,$0c
 .BYTE $18,$18,$19,$19,$7d,$7d,$dc,$dc
 .BYTE $dc,$dc,$c8,$c8,$dd,$dd,$0f,$0f
 .BYTE $63,$63,$c0,$c0,$60,$60
; FRAME 2
 .BYTE $00,$00,$0c,$0c,$0c,$0c,$0c,$0c
 .BYTE $18,$18,$18,$18,$38,$38,$5a,$5a
 .BYTE $5a,$5a,$6c,$6c,$70,$70,$2c,$2c
 .BYTE $34,$34,$32,$32,$18,$18
; FRAME 3
 .BYTE $00,$00,$0c,$0c,$0c,$0c,$0c,$0c
 .BYTE $18,$18,$18,$18,$38,$38,$58,$58
 .BYTE $5e,$5e,$60,$60,$3c,$3c,$32,$72
 .BYTE $c6,$c6,$86,$86,$07,$07
; FRAME 4
 .BYTE $00,$00,$0c,$0c,$0c,$0c,$0c,$0c
 .BYTE $18,$18,$18,$18,$38,$38,$5a,$5a
 .BYTE $5a,$5a,$6c,$6c,$70,$70,$2c,$2c
 .BYTE $34,$34,$32,$32,$18,$18
P1DATA:
; FRAME 1
 .BYTE $1c,$1c,$3c,$38,$38,$3c,$3c,$3c
 .BYTE $18,$18,$01,$01,$03,$03,$22,$20
 .BYTE $a0,$a0,$b0,$b0,$e0,$e0,$30,$30
 .BYTE $10,$00,$00,$00,$00,$00
; FRAME 2
 .BYTE $1c,$1c,$3c,$38,$38,$3c,$3c,$3c
 .BYTE $18,$18,$00,$00,$04,$04,$24,$24
 .BYTE $26,$26,$50,$50,$6c,$6c,$10,$10
 .BYTE $08,$08,$0c,$08,$00,$00
; FRAME 3
 .BYTE $1c,$1c,$3c,$38,$38,$3c,$3c,$3c
 .BYTE $18,$18,$00,$04,$04,$04,$24,$24
 .BYTE $26,$26,$5c,$5c,$02,$02,$cc,$8c
 .BYTE $30,$30,$00,$00,$00,$00
; FRAME 4
 .BYTE $1c,$1c,$3c,$38,$38,$3c,$3c,$3c
 .BYTE $18,$18,$00,$00,$04,$04,$24,$24
 .BYTE $26,$26,$50,$50,$6c,$6c,$10,$10
 .BYTE $08,$08,$0c,$08,$00,$00

