  },
  {
    0x00, 0x00, 0x0c, 0x0c, 0x0c, 0x0c, 0x0c, 0x0c, 
    0x18, 0x18, 0x18, 0x18, 0x38, 0x38, 0x5a, 0x5a, 
    0x5a, 0x5a, 0x6c, 0x6c, 0x70, 0x70, 0x2c, 0x2c, 
    0x34, 0x34, 0x32, 0x32, 0x18, 0x18
  },
  {
    0x00, 0x00, 0x0c, 0x0c, 0x0c, 0x0c, 0x0c, 0x0c, 
    0x18, 0x18, 0x18, 0x18, 0x38, 0x38, 0x58, 0x58, 
    0x5e, 0x5e, 0x60, 0x60, 0x3c, 0x3c, 0x32, 0x72, 
    0xc6, 0xc6, 0x86, 0x86, 0x07, 0x07
  },
  {
    0x00, 0x00, 0x0c, 0x0c, 0x0c, 0x0c, 0x0c, 0x0c, 
    0x18, 0x18, 0x18, 0x18, 0x38, 0x38, 0x5a, 0x5a, 
    0x5a, 0x5a, 0x6c, 0x6c, 0x70, 0x70, 0x2c, 0x2c, 
    0x34, 0x34, 0x32, 0x32, 0x18, 0x18
  }
};
